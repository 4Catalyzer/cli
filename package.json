{
  "name": "@4c/cli",
  "version": "0.1.7",
  "main": "index.js",
  "repository": "https://github.com/4Catalyzer/init.git",
  "author": "4Catalyzer",
  "license": "MIT",
  "bin": {
    "4c": "index.js"
  },
  "scripts": {
    "lint": "eslint . && prettier --list-different --ignore-path .ignore '**/*.{json,css,md}'",
    "precommit": "lint-staged"
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "importSort": {
    ".js": {
      "parser": "4catalyzer",
      "style": "4catalyzer"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.39",
    "@babel/core": "^7.0.0-beta.39",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.16.0",
    "eslint-config-4catalyzer": "^0.3.3",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-prettier": "^2.5.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "execa": "^0.9.0",
    "fs-extra": "^5.0.0",
    "inquirer": "^5.0.1",
    "ora": "^1.4.0",
    "semver": "^5.5.0",
    "slash": "^1.0.0",
    "yargs": "^11.0.0"
  }
}
