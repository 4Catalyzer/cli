{
  "plugins": ["prettier"],
  "extends": [{{#if (eq type 'web') }}"4catalyzer-react", {{else}}"4catalyzer", {{/if}}"4catalyzer-jest", "prettier"{{#if (eq type 'web') }}, "prettier-react"{{/if}}{{~#if typescript}}, "4catalyzer-typescript"{{/if}}],
  "env": {
      {{#if (eq type "node")}}
      "node": true
      {{else}}
      "browser": true
      {{/if}}
  },
  "rules": {
    "prettier/prettier": "error",
    {{#if (eq type "node")}}
    "global-require": "off",
    "import/no-dynamic-require": "off"
    {{/if}}
  }
}
